<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Admin.OwnersLaundryMenu">

    <Button
        android:id="@+id/button_employees"
        android:layout_width="215dp"
        android:layout_height="90dp"
        android:layout_marginTop="101dp"
        android:onClick="adminDeliveryOrderClick"
        android:text="Scheduled appointments "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/DeliveriesAreWelcome"
        app:layout_constraintVertical_bias="0.0"
        tools:text="Scheduled appointments " />

    <Button
        android:id="@+id/button_logout"
        android:layout_width="215dp"
        android:layout_height="90dp"
        android:layout_marginTop="105dp"
        android:onClick="logout"
        android:text="Logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/button_employees"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/ProfileM"
        android:layout_width="214dp"
        android:layout_height="90dp"
        android:onClick="adminProfileClick"
        android:text="Profile"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.369" />

    <Button
        android:id="@+id/DeliveriesAreWelcome"
        android:layout_width="214dp"
        android:layout_height="90dp"
        android:layout_marginTop="97dp"
        android:onClick="turnsOrderedClick"
        android:text="Turns ordered"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ProfileM"
        app:layout_constraintVertical_bias="0.009"
        tools:text="Turns ordered" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="316dp"
        android:layout_height="144dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.589"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.081"
        app:srcCompat="@drawable/android" />

</androidx.constraintlayout.widget.ConstraintLayout>